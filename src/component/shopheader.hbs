
  {{#with navigation}}
    <div class='card'>
      <div class='nav'>
        {{#each links}}
          <div class='nav-item'>
            <a
              href='{{href}} '
              class='nav-link {{#if @last}} disabled{{/if}}'
            >{{text}}</a>
          </div>
        {{/each}}
      </div>
    </div>
  {{/with}}

<br />